@model Domain_GestaoHospitalar.Models.Paciente

@{
    ViewData["Title"] = "Detalhes do Paciente";
}

<div class="row">
    <div class="panel panel-heading">
        <h3>@ViewData["Title"]</h3>
    </div>
    <div class="panel panel-body">
        <div>
            <partial name="_Delete_Details"/>
        </div>
        <div>
            <a asp-action="Edit" asp-route-id="@Model.Id" class="btn btn-primary">Editar Paciente</a>
            <a asp-action="Index" class="btn btn-default">Voltar para Lista</a>
        </div>
    </div>
</div>