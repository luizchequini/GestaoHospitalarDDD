@model Domain_GestaoHospitalar.Models.Paciente

@{
    ViewData["Title"] = "Details";
}

<div class="row">
    <div class="panel panel-heading">
        <h3>Detalhes do Paciente</h3>
    </div>
    <div class="panel panel-body">
        <div>
            <partial name="_Delete_Details"/>
        </div>
        <div>
            <a asp-controller="Pacientes" asp-action="Edit" asp-route-id="@Model.Id" class="btn btn-primary">Editar</a>
            <a asp-controller="Pacientes" asp-action="Index" class="btn btn-default">Voltar para Lista</a>
        </div>
    </div>
</div>